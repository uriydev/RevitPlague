<Window x:Class="RevitPlague.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:RevitPlague.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100" />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        <StackPanel Grid.Column="0">
            <Button Content="Home" Height="25" Command="{Binding NavigateToHomeCommand}" />
            <Button Content="Settings(Select)" Height="25" Command="{Binding NavigateToSettingsCommand}" />
        </StackPanel>

        <Frame Name="MainFrame" Grid.Column="1" />
    </Grid>
</Window>
